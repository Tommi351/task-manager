{
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "scripts": {
    "start": "cd backend && node server.js",
    "build": "cd frontend && npm run build",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": " cd backend && node server.js",
    "client:dev": "cd frontend && npm run dev"
  }
}
